  <DataSets>
    <DataSet Name="rpt_systems_with_hl7">
      <Query>
        <DataSourceName>VSuiteDB</DataSourceName>
        <CommandText>SELECT
	a.id,
	a.healthsystemname,
	a.abbrv

FROM sthealthsystem a (NOLOCK)

WHERE a.is_hl7_enabled = 1
	AND ISNULL(a.deleted, 0) = 0
	AND ISNULL(a.disabled, 0) = 0</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="healthsystemname">
          <DataField>healthsystemname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="abbrv">
          <DataField>abbrv</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
