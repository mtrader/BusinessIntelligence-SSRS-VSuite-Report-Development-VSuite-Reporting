    <DataSet Name="evidence_types">
      <Query>
        <DataSourceName>VSuiteDB</DataSourceName>
        <CommandText>SELECT
	Id,
	Name

FROM tblEvidenceType (NOLOCK)

WHERE Name NOT LIKE 'V%'

ORDER BY SortOrder</CommandText>
      </Query>
