  <DataSets>
    <DataSet Name="evidence_search">
      <Query>
        <DataSourceName>VSuiteDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Filter_DocumentSearch">
            <Value>=Parameters!Filter_DocumentSearch.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Filter_HealthSystem">
            <Value>=Parameters!Filter_HealthSystem.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Filter_Organization">
            <Value>=Parameters!Filter_Organization.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Filter_EvidenceType">
            <Value>=Parameters!Filter_EvidenceType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @searchText varchar(512) = '%' + @Filter_DocumentSearch + '%'

SELECT
	a.id			evidence_id,
	a.title			title,
	CASE 
	  WHEN a.status = 0
	  THEN 'Active'

  	  WHEN a.status = 1
	  THEN 'Inactive'

	  ELSE NULL

	END AS			status,
	a.status			status_id, 	
	a.description		category,
	a.HCO			organization,
	a.neventmanager		owner,
	a.EvidenceCreatorName 	creator,
	a.ondate			created_date,
	a.lastupdated		last_updated_date,
	b.healthsystemname	health_system,
	c.Name			evidence_type

FROM vwEvidence a
	JOIN sthealthsystem b (NOLOCK) ON a.SystemId = b.id
	JOIN tblEvidenceType c (NOLOCK) ON a.eventtype = c.id

WHERE a.title LIKE @searchText
	AND (a.SystemId IN (SELECT value FROM dbo.fn_Split(@Filter_HealthSystem, ',')) OR @Filter_HealthSystem = -1)
	
	AND (a.vhco_id IN (SELECT value FROM dbo.fn_Split(@Filter_Organization, ',')) OR @Filter_Organization = -1)

	AND (a.eventtype IN (SELECT value FROM dbo.fn_Split(@Filter_EvidenceType, ',')))
	
	
	
	</CommandText>
      </Query>
      <Fields>
        <Field Name="title">
          <DataField>title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="evidence_id">
          <DataField>evidence_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="organization">
          <DataField>organization</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
