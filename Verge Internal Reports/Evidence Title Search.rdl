    <DataSet Name="health_systems">
      <Query>
        <DataSourceName>VSuiteDB</DataSourceName>
        <CommandText>SELECT
	id, 
	healthsystemname,
	healthsystemname 	sort

FROM sthealthsystem 

WHERE is_deleted = 0
	AND disabled = 0
	AND ISNUMERIC(abbrv) = 0

UNION ALL 
	SELECT -1, 'All', 'aaaaaaa'

ORDER BY sort</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="healthsystemname">
          <DataField>healthsystemname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sort">
          <DataField>sort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="orgs">
      <Query>
        <DataSourceName>VSuiteDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Filter_HealthSystem">
            <Value>=Parameters!Filter_HealthSystem.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
	b.id,
	b.HCO,
	b.HCO	sort

FROM sthcohealthsystem a
 JOIN vhco b (NOLOCK) ON a.vhco_id = b.id 

WHERE a.sthealthsystem_id IN (SELECT value FROM dbo.fn_Split(@Filter_HealthSystem, ','))

UNION ALL
	SELECT -1, 'All', 'aaaaaa'

ORDER BY sort</CommandText>
      </Query>
      <Fields>
        <Field Name="id">
