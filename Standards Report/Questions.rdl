  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>VSuite_DB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OrgElementId">
            <Value>=Parameters!OrgElementId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Run">
            <Value>=Parameters!Run.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FilterByAuditData">
            <Value>=Parameters!FilterByAuditData.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF @Run = 1 BEGIN IF @FilterByAuditData = 1 
BEGIN 
SELECT description, count(Id) AS NumAudits, OrgElementId, SUM(answeryes) AS AnswerYes, Sum(answerno) as AnswerNo, 
SUM(answeryes) + Sum(answerno) AS Total
FROM            dbo.vwOrgElementQuestionReport
WHERE        OrgElementId = @OrgElementId AND AuditDate &gt;
(SELECT        DATEADD(DD, - 90, CURRENT_TIMESTAMP))
GROUP BY description, OrgElementId 
END 
ELSE 
BEGIN
 SELECT        description, count(Id) AS NumAudits, OrgElementId, SUM(answeryes) AS AnswerYes, 
Sum(answerno) AS AnswerNo, SUM(answeryes) + Sum(answerno) AS Total
FROM            dbo.vwOrgElementQuestionReport
--WHERE        (OrgElementId = @OrgElementId AND AuditDate &gt;
--(SELECT        DATEADD(DD, - 90, CURRENT_TIMESTAMP))) OR
--(OrgElementId = @OrgElementId AND id IS NULL)   
WHERE OrgElementId = @OrgElementId
GROUP BY description, OrgElementId 
END 
END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
